{
	"root": true,
	"extends": [
		"wikimedia",
		"wikimedia/mediawiki"
	],
	"env": {
		"browser": true,
		"jquery": true,
		"commonjs": true
	},
	"globals": {
		"mediaWiki": false,
		"wikibase": false
	},
	"rules": {
		"compat/compat": "error",
		"max-len": "off",
		"no-underscore-dangle": "off"
	},
	"plugins": [
		"compat"
	]
}
