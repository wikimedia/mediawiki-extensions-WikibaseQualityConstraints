{
	"extends": "wikimedia",
	"env": {
		"browser": true,
		"jquery": true
	},
	"globals": {
		"module": false,
		"mediaWiki": false,
		"wikibase": false,
		"OO": false
	},
	"rules": {
		"compat/compat": "error",
		"dot-notation": [ "error", { "allowKeywords": true } ],
		"no-underscore-dangle": "off"
	},
	"plugins": ["compat"]
}
